   "source": [
    "import math\n",
    "def f(z):\n",
    "    return (math.exp(-0.5*z**2)/((2.0*math.pi)**0.5))\n",
    "n=10\n",
    "a=-5.0\n",
    "b=0.0\n",
    "h=(b-a)/n\n",
    "sum=0.5*(f(a)+f(b))\n",
    "for i in range(1,n):\n",
    "    x=a+i*h\n",
    "    sum=sum+f(x)\n",
    "sum=sum*h\n",
    "print(\"The area is {} after {} iterations\".format(sum,n))"
   ]
